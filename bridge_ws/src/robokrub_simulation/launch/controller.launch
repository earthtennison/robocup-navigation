<launch>

<rosparam file="$(find robokrub_base_description)/launch/controller.yaml" command="load"/>
<node args="Rev9_position_controller Rev10_position_controller Rev11_position_controller Rev12_position_controller joint_state_controller " name="controller_spawner" ns="robokrub_base" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/robokrub_base/joint_states"/>
</node>

</launch>